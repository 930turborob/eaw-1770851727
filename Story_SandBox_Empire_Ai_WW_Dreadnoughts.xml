<Story>

<!-- Piett Respawn -->
	<Event Name="Piett_Death">
		<Event_Type>STORY_TACTICAL_DESTROY</Event_Type>
		<Event_Param1>Imperial_Piett_Executor_P1</Event_Param1>
		<Reward_Type>REMOVE_UNIT</Reward_Type>
