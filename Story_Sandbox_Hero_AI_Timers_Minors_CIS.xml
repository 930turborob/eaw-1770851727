<?xml version="1.0" ?>
<Story>
	
    <Event Name="Story_Start">
        <Event_Type>STORY_ELAPSED</Event_Type>
        <Event_Param1>0</Event_Param1>
    </Event>

<!-- Hero Respawns -->

<!-- Zann -->
	<Event Name="Vooro_Death">
		<Event_Type>STORY_TACTICAL_DESTROY</Event_Type>
		<Event_Param1>Great_Rapina</Event_Param1>
		<Branch>Vooro_Death_Branch</Branch>
		<Perpetual>True</Perpetual>
		<Story_Dialog_Popup>FALSE</Story_Dialog_Popup>
	</Event>
	<Event Name="Vooro_Death_Spawn">
		<Event_Type>STORY_ELAPSED</Event_Type>
		<Event_Param1>1800</Event_Param1>
		<Reward_Type>SPAWN_HERO</Reward_Type>
		<Reward_Param1>Great_Rapina</Reward_Param1>
		<Reward_Param2>Mustafar_WW</Reward_Param2>
		<Prereq>Vooro_Death</Prereq>
		<Branch>Vooro_Death_Branch</Branch>
	</Event>
	<Event Name="Vooro_Death_Spawn_Reset">
		<Event_Type>STORY_TRIGGER</Event_Type>
		<Reward_Type>RESET_BRANCH</Reward_Type>
		<Reward_Param1>Vooro_Death_Branch</Reward_Param1>
		<Prereq>Vooro_Death_Spawn</Prereq>
		<Branch>Vooro_Death_Branch</Branch>
		<Perpetual>true</Perpetual>
	</Event>


